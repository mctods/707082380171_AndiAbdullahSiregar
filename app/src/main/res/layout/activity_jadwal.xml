<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".jadwal"
    android:background="#809506e5">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:background="@drawable/psychedelicheader"
        android:orientation="vertical">
        <!--tools:context=".home"> -->
    </FrameLayout>
    <LinearLayout
        android:id="@+id/kotakbiru"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:cardCornerRadius="15dp"
            android:orientation="vertical"
            android:backgroundTint="#90000000"
            android:layout_marginLeft="5dp">

            <TextView
                android:id="@+id/header_history"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/psychedelicfont"
                android:layout_marginTop="5dp"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:text="JADWAL"
                android:textColor="#f7e001"
                android:textSize="18dp" />
        </androidx.cardview.widget.CardView>


        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_marginLeft="230dp"
            android:src="@drawable/fotoprofil"
            android:layout_marginTop="10dp"/>
    </LinearLayout>
    <androidx.cardview.widget.CardView
        android:layout_width="390dp"
        android:layout_height="500dp"
        android:layout_marginTop="100dp"
        app:cardCornerRadius="20dp"
        android:layout_gravity="center_horizontal">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginTop="150dp"
                android:text="COMING SOON !!!"
                android:textSize="70dp"/>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>
</FrameLayout>